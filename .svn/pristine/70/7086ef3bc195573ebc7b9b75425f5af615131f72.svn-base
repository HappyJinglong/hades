package com.flyrish.hades.exception;

import org.acegisecurity.AuthenticationException;

public class UserTermIsUsedException extends AuthenticationException{

	/**
	 * 
	 * @param msg
	 */
	public UserTermIsUsedException(String msg) {
		super(msg);
	}

	/**
	 * 
	 * @param msg
	 * @param t
	 */
	public UserTermIsUsedException(String msg, Throwable t) {
		super(msg, t);
	}
}
