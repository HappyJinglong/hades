							inner join e_class ecl on eg.gradeid=ecl.gradeid and ecl.used=1 
							inner join e_stuclass est on est.classid=ecl.classid and est.used=1
							inner join s_baseinfo sb on sb.studentid=est.studentid and sb.inschoolstate='1002001' and sb.discode=:discode
             				inner join b_baseinfo bb on bb.cmis30id=sb.cmis30id 
             				<#if schoolname?has_content>
              				and bb.schoolname like %:schoolname%
              				</#if>
							inner join sh_healthstandard sa on sa.studentid=sb.studentid and sa.schoolyear=substr(:termid,1,4)
              				and sa.discode=:discode
							inner join sh_healthdata shha on sa.healthstdid=shha.healthstdid and shha.discode=:discode
							where eg.used=1 and eg.gradenum>0 and to_char(eg.graduateyear,'yyyy')=:gradeyear 
							<#if levelcode == '2012002'>
							<#else>
								and eg.gradenum<4
							</#if>
							group by bb.schoolname
						]]></value>
				</entry>
				<entry key="StudentAppDetailExtImpl.queryDiscodeTotailYDYJKBaseData.query">
					<value><![CDATA[
							select  bb.schoolname,count(distinct sb.studentid) couts,count(distinct sa.healthstdid) sacouts from e_grade eg 
							inner join e_term et on et.used=1 and et.schoolyear=eg.schoolyear
              				inner join e_edusys ee on ee.levelid=eg.levelid and ee.used=1 
              				<#if levelcode == '2012002'>
								and ee.levelcode=2012002
							<#else>
								and ee.levelcode in (2012003,2012004)
							</#if>
							inner join e_class ecl on eg.gradeid=ecl.gradeid and ecl.used=1 
							inner join e_stuclass est on est.classid=ecl.classid and est.used=1
							inner join s_baseinfo sb on sb.studentid=est.studentid and sb.inschoolstate='1002001' and sb.discode=:discode
             				inner join b_baseinfo bb on bb.cmis30id=sb.cmis30id 
             				<#if schoolname?has_content>
              				and bb.schoolname like %:schoolname%
              				</#if>
							inner join sh_healthstandard sa on sa.studentid=sb.studentid and sa.schoolyear=substr(:termid,1,4)
              				and sa.discode=:discode
							inner join sh_healthdata shha on sa.healthstdid=shha.healthstdid and shha.discode=:discode
							where eg.used=1 and eg.gradenum>0 and to_char(eg.graduateyear,'yyyy')=:gradeyear 
							<#if levelcode == '2012002'>
							<#else>
							and eg.gradenum<4
							</#if>
							group by bb.schoolname
						]]></value>
				</entry>
				<entry key="TermServiceExtImpl.queryHighSchoolTerms.query2">
					<value><![CDATA[
							select termid,termname from e_term where schoolyear>=:gradeYearInt and schoolyear<=(
								select schoolyear from e_term where used=1
							) order by termid desc
						]]></value>
				</entry>
				<entry key="StudentAppDetailExtImpl.querySchoolStudetNums.query">
					<value><![CDATA[
							select bb.schoolname,count(distinct sb.studentid) couts from s_baseinfo sb
							inner join e_stuclass est on est.studentid=sb.studentid and est.used=1
							inner join e_class ecl on ecl.classid=est.classid and ecl.used=1
							inner join e_grade eg on eg.gradeid=ecl.gradeid and eg.used=1 and to_char(eg.graduateyear,'yyyy')=:gradeyear
							inner join e_edusys ee on ee.levelid=eg.levelid and ee.used=1 
							<#if levelcode == '2012002'>
								and ee.levelcode=2012002
							<#else>
								and ee.levelcode in (2012003,2012004)
							</#if>
							inner join e_term et on et.schoolyear=eg.schoolyear and et.used=1
							inner join e_term oet on to_char(eg.graduateyear,'yyyy')<=oet.schoolyear+ee.length and oet.termid=:termid
							inner join b_baseinfo bb on bb.cmis30id=sb.cmis30id 
							<#if schoolname?has_content>
              				and bb.schoolname like %:schoolname%
              				</#if>
              				and bb.cmis30id not in(
					         	select distinct md.mergedschoolcmis30id from b_merge bm
								inner join mergedschool md on md.mergeid=bm.mergeid  where  bm.checkstate='1550040'
					         )
							where sb.inschoolstate='1002001' and sb.discode=:discode
							group by bb.schoolname
						]]></value>
				</entry>
				<entry key="StudentAppDetailExtImpl.querySemterid.query">
					<value><![CDATA[
								select distinct (eg.gradenum-et.schoolyear+substr(:termid,0,4))||(decode(mod(:termid,2),0,2,1,1)) semtemid from e_grade eg
								inner join e_edusys ee on ee.levelid=eg.levelid and ee.levelcode='2012003' and ee.used=1
								inner join e_term et on et.schoolyear=eg.schoolyear and et.used=1
								where to_char(eg.graduateyear,'yyyy')=:gradeyear and eg.gradenum>0 
								and eg.used=1 and eg.gradenum<4 and (eg.gradenum-et.schoolyear+substr(:termid,0,4))>0
						]]></value>
				</entry>
				<entry key="StudentAppDetailExtImpl.queryDiscodeAppraiseData.query">
					<value><![CDATA[
							select  bb.schoolname,aa.appraisaltypeid appraisaltypeid,count(distinct sb.studentid) couts from e_grade eg 
							inner join e_term et on et.used=1 and et.schoolyear=eg.schoolyear
              				inner join e_edusys ee on ee.levelid=eg.levelid and ee.used=1 
              				<#if levelcode == '2012002'>
								and ee.levelcode=2012002
							<#else>
								and ee.levelcode in (2012003,2012004)
							</#if>
							inner join e_class ecl on eg.gradeid=ecl.gradeid and ecl.used=1 
							inner join e_stuclass est on est.classid=ecl.classid and est.used=1
							inner join s_baseinfo sb on sb.studentid=est.studentid and sb.inschoolstate='1002001' and sb.discode=:discode
              				inner join b_baseinfo bb on bb.cmis30id=sb.cmis30id 
              				<#if schoolname?has_content>
              				and bb.schoolname like %:schoolname%
              				</#if>
							inner join a_apprasial aa on aa.edu_id=sb.edu_id and aa.semesterid=:termid
							and (aa.appraisaltypeid=1010 or aa.appraisaltypeid=1020 and aa.appraseridentify=1
							or aa.appraisaltypeid=2010 or aa.appraisaltypeid=7050 or aa.appraisaltypeid=2040 
              				or aa.appraisaltypeid=3010 or aa.appraisaltypeid=4020 and aa.appraseridentify=2)
							where eg.used=1 and eg.gradenum>0 and to_char(eg.graduateyear,'yyyy')=:gradeyear and eg.gradenum<4
							group by bb.schoolname,aa.appraisaltypeid,eg.graduateyear
						]]></value>
				</entry>
				<entry key="StudentAppDetailExtImpl.queryDiscodeHighTotalAppraiseData.query">
					<value><![CDATA[
							select  
								bb.schoolname,
								aa.appraisaltypeid appraisaltypeid,
								case when aa.appraseridentify=4 then 3 else aa.appraseridentify end as appraseridentify,
								count(distinct sb.studentid) stucouts,
								count(distinct aa.apprasialid) apcouts,count(distinct at.attachid) attcounts from e_grade eg 
							inner join e_term et on et.used=1 and et.schoolyear=eg.schoolyear
              				inner join e_edusys ee on ee.levelid=eg.levelid and ee.used=1 
							and ee.levelcode in (2012003,2012004)
							inner join e_class ecl on eg.gradeid=ecl.gradeid and ecl.used=1 
							inner join e_stuclass est on est.classid=ecl.classid and est.used=1
							inner join s_baseinfo sb on sb.studentid=est.studentid and sb.inschoolstate='1002001' and sb.discode=:discode
			              	inner join b_baseinfo bb on bb.cmis30id=sb.cmis30id 
			              	<#if schoolname?has_content>
			                    and bb.schoolname like %:schoolname%
			             	</#if>
							inner join a_apprasial aa on aa.edu_id=sb.edu_id and aa.semesterid=:termid
							left join a_attach at on at.attachtypeid=aa.apprasialid and at.attachtype=2
							where eg.used=1 and eg.gradenum>0 and to_char(eg.graduateyear,'yyyy')=:gradeyear and eg.gradenum<4
							group by bb.schoolname,aa.appraisaltypeid,case when aa.appraseridentify=4 then 3 else aa.appraseridentify end
						]]></value>
				</entry>
				<entry key="IScoreExt.initClassModelAndSegmentModel.query">
					<value><![CDATA[
							select ksm.sement_model_id from k_segment_model ksm
							inner join k_study_segment kss on kss.schoolyear=:schoolyearName and kss.cmis30id=:cmis30id and kss.segment_order=:segmentName
							and kss.segment_id=ksm.segment_id
							where ksm.course_id=:course_id
						]]></value>
				</entry>
				<entry key="IScoreExt.querySementModelidIsExist.add">
					<value><![CDATA[
							insert into k_segment_model(sement_model_id,segment_id,course_id,is_assign)
							select :sement_model_id,segment_id,:course_id,'1' from k_study_segment where schoolyear=:schoolyearName and cmis30id=:cmis30id and segment_order=:segmentName
						]]></value>
				</entry>
				<entry key="IScoreExt.isExistClassModel.query">
					<value><![CDATA[
							select class_model_id from k_class_model where 
							<#if historyclassid?has_content>
								classid=:historyclassid 
							<#else>
								classid=:classid 
								and schoolyear=:schoolyear
							</#if>
							and sement_model_id=:sement_model_id
						]]></value>
				</entry>
				<entry key="IScoreExt.generateClassModel.add">
					<value><![CDATA[
							insert into k_class_model(class_model_id,sement_model_id,teacherid,classid,schoolyear,need_assign,is_assigned,assign_model,special)
							values (:class_model_id,:sement_model_id,:teacherid,:classid,:schoolyear,:need_assign,:is_assigned,:assign_model,:special)
						]]></value>
				</entry>
				<entry key="IScoreExt.getProKey.query">
					<value><![CDATA[
							select sys_guid() class_model_id from dual
						]]></value>
				</entry>
				<entry key="StudentAppDetailExtImpl.queryDiscodeHighTotalRecordpackageData.query">
					<value><![CDATA[
							select  bb.schoolname,aa.appraisaltypeid appraisaltypeid,count(distinct sb.studentid) stucounts,count(distinct aa.recordid) apcouts,count(distinct at.attachid) attachcount from e_grade eg 
							inner join e_term et on et.used=1 and et.schoolyear=eg.schoolyear
              				inner join e_edusys ee on ee.levelid=eg.levelid and ee.used=1 
								and ee.levelcode in (2012003,2012004)
							inner join e_class ecl on eg.gradeid=ecl.gradeid and ecl.used=1 
							inner join e_stuclass est on est.classid=ecl.classid and est.used=1
							inner join s_baseinfo sb on sb.studentid=est.studentid and sb.inschoolstate='1002001' and sb.discode=:discode
              				inner join b_baseinfo bb on bb.cmis30id=sb.cmis30id 
              				<#if schoolname?has_content>
                   				 and bb.schoolname like %:schoolname%
             				 </#if>
							inner join a_recordpackage aa on aa.edu_id=sb.edu_id and aa.semesterid=:termid and aa.appraseridentify=1
							left join a_attach at on at.attachtypeid=aa.recordid and at.attachtype=1
							where eg.used=1 and eg.gradenum>0 and to_char(eg.graduateyear,'yyyy')=:gradeyear and eg.gradenum<4
							group by bb.schoolname,aa.appraisaltypeid
						]]></value>
				</entry>
				<entry key="StudentAppDetailExtImpl.queryDiscodeHighTotalPracticesData.query">
					<value><![CDATA[
							select  bb.schoolname,aa.appraisaltypeid appraisaltypeid,count(distinct sb.studentid) stucouts,count(distinct aa.practiceid) apcouts,count(distinct at.attachid) atcounts from e_grade eg 
							inner join e_term et on et.used=1 and et.schoolyear=eg.schoolyear
              				inner join e_edusys ee on ee.levelid=eg.levelid and ee.used=1 and ee.levelcode in (2012003,2012004)
							inner join e_class ecl on eg.gradeid=ecl.gradeid and ecl.used=1 
							inner join e_stuclass est on est.classid=ecl.classid and est.used=1
							inner join s_baseinfo sb on sb.studentid=est.studentid and sb.inschoolstate='1002001' and sb.discode=:discode
				            inner join b_baseinfo bb on bb.cmis30id=sb.cmis30id 
				              <#if schoolname?has_content>
				                    and bb.schoolname like %:schoolname%
				              </#if>
							inner join a_practices aa on aa.edu_id=sb.edu_id and aa.semesterid=:termid
							left join a_attach at on at.attachtypeid=aa.practiceid and at.attachtype=3
							where eg.used=1 and eg.gradenum>0 and to_char(eg.graduateyear,'yyyy')=:gradeyear and eg.gradenum<4
							group by bb.schoolname,aa.appraisaltypeid
						]]></value>
				</entry>
				<entry key="StudentAppDetailExtImpl.queryDiscodeHighTotalPersonBaseData.query">
					<value><![CDATA[
							select  bb.schoolname,count(distinct aa.baseid) apcouts,count(distinct sb.studentid) stucounts,count(distinct at.attachid) attachcount from e_grade eg 
							inner join e_term et on et.used=1 and et.schoolyear=eg.schoolyear
              				inner join e_edusys ee on ee.levelid=eg.levelid and ee.used=1 
								and ee.levelcode in (2012003,2012004)
							inner join e_class ecl on eg.gradeid=ecl.gradeid and ecl.used=1 
							inner join e_stuclass est on est.classid=ecl.classid and est.used=1
							inner join s_baseinfo sb on sb.studentid=est.studentid and sb.inschoolstate='1002001' and sb.discode=:discode
				            inner join b_baseinfo bb on bb.cmis30id=sb.cmis30id 
				              <#if schoolname?has_content>
				                    and bb.schoolname like %:schoolname%
				              </#if>
							inner join a_personality aa on aa.edu_id=sb.edu_id and aa.semesterid=:termid
							left join a_attach at on at.attachtypeid=aa.baseid and at.attachtype=2
							where eg.used=1 and eg.gradenum>0 and to_char(eg.graduateyear,'yyyy')=:gradeyear and eg.gradenum<4
							group by bb.schoolname
						]]></value>
				</entry>
				<entry key="StudentAppDetailExtImpl.queryDiscodeHighTotalLearnProcessData.query">
					<value><![CDATA[
							select  bb.schoolname,count(distinct sb.studentid) stucouts,count(distinct aa.workid) apcouts,count(distinct at.attachid) atcounts from e_grade eg 
							inner join e_term et on et.used=1 and et.schoolyear=eg.schoolyear
              				inner join e_edusys ee on ee.levelid=eg.levelid and ee.used=1 
								and ee.levelcode in (2012003,2012004)
							inner join e_class ecl on eg.gradeid=ecl.gradeid and ecl.used=1 
							inner join e_stuclass est on est.classid=ecl.classid and est.used=1
							inner join s_baseinfo sb on sb.studentid=est.studentid and sb.inschoolstate='1002001' and sb.discode=:discode
				              inner join b_baseinfo bb on bb.cmis30id=sb.cmis30id 
				              <#if schoolname?has_content>
				                    and bb.schoolname like %:schoolname%
				              </#if>
							inner join a_learnprocess_works aa on aa.edu_id=sb.edu_id and aa.semesterid=:termid
							left join a_attach at on at.attachtypeid=aa.workid and at.attachtype=4
							where eg.used=1 and eg.gradenum>0 and to_char(eg.graduateyear,'yyyy')=:gradeyear and eg.gradenum<4
							group by bb.schoolname
						]]></value>
				</entry>
				<entry key="StudentAppDetailExtImpl.queryDiscodeHighTotalLearnprocessAppraisal.query">
					<value><![CDATA[
							select  bb.schoolname,count(distinct sb.studentid) stucouts,count(distinct aa.appraisalid) apcouts from e_grade eg 
							inner join e_term et on et.used=1 and et.schoolyear=eg.schoolyear
              				inner join e_edusys ee on ee.levelid=eg.levelid and ee.used=1 
								and ee.levelcode in (2012003,2012004)
							inner join e_class ecl on eg.gradeid=ecl.gradeid and ecl.used=1 
							inner join e_stuclass est on est.classid=ecl.classid and est.used=1
							inner join s_baseinfo sb on sb.studentid=est.studentid and sb.inschoolstate='1002001' and sb.discode=:discode
				              inner join b_baseinfo bb on bb.cmis30id=sb.cmis30id 
				              <#if schoolname?has_content>
				                    and bb.schoolname like %:schoolname%
				              </#if>
							inner join a_learnprocess_appraisal aa on aa.edu_id=sb.edu_id and aa.semesterid=:termid
							where eg.used=1 and eg.gradenum>0 and to_char(eg.graduateyear,'yyyy')=:gradeyear and eg.gradenum<4
							group by bb.schoolname
						]]></value>
				</entry>
				<entry key="IScoreExt.queryClassModelAndCreditByCondition.query">
					<value><![CDATA[
							select distinct kcm.class_model_id,k.credit_id
							from k_class_model kcm
							inner join k_study_segment kss on 1=1 and kss.cmis30id=:cmis30id and kss.schoolyear=:schoolyear and kss.segment_order=:segment_order
							inner join k_segment_model ksm on ksm.segment_id=kss.segment_id and	kcm.sement_model_id=ksm.sement_model_id and ksm.course_id=:course_id
							left join k_student_model k on  k.class_model_id=kcm.class_model_id and k.discode=:discode and k.cmis30id=:cmis30id and k.partid=mod(:cmis30id,20) and k.studentid=:studentid
							where 1=1 
							<#if historyclassid?has_content>
								and kcm.classid=:historyclassid 
							<#else>
								and kcm.classid=:classid 
								and kcm.schoolyear=:schoolyear
							</#if>
						]]></value>
				</entry>
			</map>	                                                            
		</property>
	</bean>
</beans>